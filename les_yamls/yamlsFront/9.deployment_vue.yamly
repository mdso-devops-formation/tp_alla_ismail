apiVersion: apps/v1
kind: Deployment
metadata:
  name: vue
spec:
  selector:
    matchLabels:
      app: vue
  template:
    metadata:
      labels:
        app: vue
    spec:
      containers:
      - name: vue
        image: allaismail/fronten
        env:
        - name: IP_API
          value: "10.64.140.43"
        - name: PORT_API
          value: "3000"
        - name: PROTOCOL
          value: "http"
        ports:
        - containerPort: 8000
